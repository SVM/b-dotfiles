rmall	b-shell/bash/bash_alias.sh	/^rmall() {$/;"	f
rmls	b-shell/bash/bash_alias.sh	/^rmls() {$/;"	f
sbs	b-shell/bash/bash_alias.sh	/^sbs(){ du -b --max-depth 1 | sort -nr | perl -pe 's{([0-9]+)}{sprintf "%.1f%s", $1>=2**30? ($1\/2**30, "G"): $1>=2**20? ($1\/2**20, "M"): $1>=2**10? ($1\/2**10, "K"): ($1, "")}e';}$/;"	f
