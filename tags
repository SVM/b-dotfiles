mcd	b-shell/bash/bash_alias.sh	/^mcd() { mkdir -p "$1"; cd "$1";}$/;"	f
md5check	b-shell/bash/bash_alias.sh	/^md5check() { md5sum "$1" | grep "$2";}$/;"	f
pcd	b-shell/bash/bash_alias.sh	/^pcd() {$/;"	f
pls	b-shell/bash/bash_alias.sh	/^pls() {$/;"	f
