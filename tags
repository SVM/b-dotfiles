backup	b-shell/bash/bash_alias.sh	/^backup() { cp "$1"{,.bak};}$/;"	f
cls	b-shell/bash/bash_alias.sh	/^cls() { cd "$1"; ls;}$/;"	f
extract	b-shell/bash/bash_alias.sh	/^extract() {$/;"	f
lnif	_oldfiles/b-powerline/install-powerline.sh	/^lnif(){$/;"	f
